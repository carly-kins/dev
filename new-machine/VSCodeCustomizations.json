            "statusBar.foreground": "accent"
        }
    },
    "indentRainbow.colors": [
        "rgba(245, 194, 231, 0.1)",
        "rgba(249, 226, 175, 0.1)",
        "rgba(166, 227, 161, 0.1)",
        "rgba(116, 199, 236, 0.1)",
        "rgba(180, 190, 254, 0.1)"
