        "siteId": "cc98bf14-ee56-4937-b77c-3736fb579d0c",
        "projectKey": "AAA"
    },
    "html.autoClosingTags": true,
    "editor.linkedEditing": true,
    "settingsSync.ignoredSettings": [],
